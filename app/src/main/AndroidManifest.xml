<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.maurosantos.android.posologia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".PessoaActivity"
            android:label="@string/app_title_pessoa" />
        <activity
            android:name=".MedicamentoActivity"
            android:label="@string/app_title_medicamento" />
        <activity
            android:name=".PosologiaActivity"
            android:label="@string/app_title_posologia" />

        <activity
            android:name=".PessoaNewActivity"
            android:label="@string/app_title_pessoa_new" />

        <activity
            android:name=".MedicamentoNewActivity"
            android:label="@string/app_title_medicamento_new" />
    </application>

</manifest>